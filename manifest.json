{
	"manifest_version": 2,
	"name": "Hours Calculator Extension",
	"short_name": "HoursCalc",
	"version": "2.0",
	"content_scripts": [
		{
			"matches": ["https://workforcenow.adp.com/*"],
			"js": ["jquery-3.2.1.min.js", "content.js"]
		}
	],
	"page_action": {
		"default_icon": "doge.png",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	}
	
}
